
<h2>Create a new interview</h2>
<%= simple_form_for([@interview]) do |f| %>
  <%= f.input :picture,
                label: "Upload uw profiel foto" %>
  <%= f.input :picture_cache, as: :hidden %>
  <%= f.input :camping %>
  <%= f.input :first_name %>
  <%= f.input :last_name %>
  <%= f.input :title %>
  <%= f.input :content, as: :trix_editor %>
  <%= f.button :submit, class: "new-reservations-btn"%>
<% end %>


